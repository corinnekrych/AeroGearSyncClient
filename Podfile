source 'https://github.com/CocoaPods/Specs.git'

xcodeproj 'AeroGearSyncClient.xcodeproj'
platform :ios, '8.0'

pod 'DiffMatchPatch', :git => "https://github.com/danbev/diffmatchpatch-ios.git", :tag => "0.1.1"
pod 'Starscream', :git => "https://github.com/danbev/starscream.git", :tag => "0.9.1"
pod 'AeroGearSync', :git => "https://github.com/danbev/aerogear-ios-sync.git", :branch => "master"
pod 'SwiftyJSON', :git => "https://github.com/danbev/SwiftyJSON.git", :branch => "cocoapods"

target 'AeroGearSyncClientTests' do
	pod 'DiffMatchPatch', :git => "https://github.com/danbev/diffmatchpatch-ios.git", :tag => "0.1.1"
    pod 'Starscream', :git => "https://github.com/danbev/starscream.git", :tag => "0.9.1"
    pod 'AeroGearSync', :git => "https://github.com/danbev/aerogear-ios-sync.git", :branch => "master"
    pod 'SwiftyJSON', :git => "https://github.com/danbev/SwiftyJSON.git", :branch => "cocoapods"
end
